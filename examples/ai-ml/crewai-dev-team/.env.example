# CrewAI Development Team Configuration
# Copy this to .env and fill in your API keys

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL_NAME=gpt-4-turbo-preview

# GitHub Configuration (for GitHub Search Tool)
GITHUB_TOKEN=your-github-token-here

# Project Configuration
PROJECT_PATH=.
REPORTS_DIR=./reports

# Crew Configuration
CREW_VERBOSE=true
CREW_MEMORY=true
CREW_MAX_EXECUTION_TIME=1800  # 30 minutes

# Agent Configuration
CODE_REVIEWER_TEMPERATURE=0.1
ARCHITECTURE_ADVISOR_TEMPERATURE=0.2
DOCUMENTATION_SPECIALIST_TEMPERATURE=0.3
DEVOPS_ENGINEER_TEMPERATURE=0.1
SECURITY_SPECIALIST_TEMPERATURE=0.1

# Output Configuration
GENERATE_MARKDOWN_REPORT=true
GENERATE_JSON_REPORT=true
GENERATE_HTML_REPORT=false